
make gen_dot_file 
	to generate the excutable that generates the dot find_in_file
	usage:
		./gen_dot_file [automate file] [output file .dot]
	
make find_word
	to generate the excutable that finds a word in the automate
	usage:
		./find_word [automate file] [word to find]

make find_in_file
	to generate the excutable that printts the words found in the file and in the automate
	usage:
		./find_in_file [automate file] [file.txt]